{
  "rules": {
    "no-console": 0,
    "import/no-extraneous-dependencies": 0,
    "consistent-return": 0,
    "no-await-in-loop": 0,
    "prefer-destructuring": [
      "error",
      {
        "VariableDeclarator": {
          "array": false,
          "object": true
        },
        "AssignmentExpression": {
          "array": false,
          "object": true
        }
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  }
}
