    "@rowno/sparkline": "^3.0.1",
    "@types/googlemaps": "^3.37.7",
    "@types/markerclustererplus": "^2.1.33",
    "antd": "^3.11.0",
    "apollo-cache-inmemory": "^1.6.2",
    "aws-amplify": "^1.1.19",
    "chai": "^4.2.0",
    "chart.js": "^2.7.3",
    "chartist": "^0.10.1",
    "chartist-plugin-tooltips-updated": "^0.0.20",
    "classnames": "^2.2.6",
    "d3-dsv": "^1.0.10",
    "d3-format": "^1.3.2",
    "d3-time-format": "^2.1.3",
    "dva": "^2.4.1",
    "elasticsearch": "^16.3.0",
    "enquire-js": "^0.2.1",
    "file-saver": "^2.0.2",
    "firebase": "^5.7.0",
    "fs": "0.0.1-security",
    "fuzzysort": "^1.1.4",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "highcharts": "^7.2.0",
    "http-aws-es": "^6.0.0",
    "js-yaml": "^3.13.1",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "new-relic-agent-react": "^2.0.1",
    "node-fetch": "^2.6.0",
    "nprogress": "^0.2.0",
    "peity-react": "^0.0.7",
    "prop-types": "^15.6.2",
    "qs": "^6.7.0",
    "randexp": "^0.5.3",
    "random-words": "^1.1.0",
    "rc-drawer": "^1.7.6",
    "react": "^16.6.3",
    "react-adal": "^0.4.22",
    "react-apollo": "^2.5.6",
    "react-c3js": "^0.1.20",
    "react-chartist": "^0.13.1",
    "react-chartjs-2": "^2.7.4",
    "react-custom-scrollbars": "^4.2.1",
