    "react-ga": "^2.6.0",
    "react-helmet": "^5.2.0",
    "react-highcharts": "^16.0.2",
    "react-intl": "^2.9.0",
    "react-jsonschema-form": "^1.6.0",
    "react-redux": "^7.1.0",
    "react-responsive-carousel": "^3.1.47",
    "react-router-breadcrumbs-hoc": "^2.3.1",
    "react-router-dom": "^4.3.1",
    "react-stockcharts": "^0.7.8",
    "react-swipeable": "^4.3.2",
    "react-webcam": "^3.0.1",
    "reams-aws-appsync": "^1.7.3",
    "reams-aws-appsync-react": "^1.2.8",
    "redux": "^4.0.1",
    "shortid": "^2.2.14",
    "sinon": "^7.3.2",
