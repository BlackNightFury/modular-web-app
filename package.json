{
  "name": "modular-web-app",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "start": "./scripts/app_script_dev.sh",
    "start:codebuild": "./scripts/app_script_dev_codebuild.js",
    "build": "./scripts/app_script_build.sh",
    "test:integration": "npx babel-node --config-file=\"./cucumber.babel.config.js\" --presets=@babel/react,@babel/preset-env node_modules/cucumber/bin/cucumber-js",
    "test": "./scripts/app_script_test.sh",
    "test:infrastructure-acceptance": "./scripts/test_infrastructure-acceptance.sh",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --ext .js,jsx ./",
    "tslint": "tslint \"src/**/*.{ts,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "serverless-deploy": "bash ./scripts/serverless.sh deploy",
    "serverless-remove": "bash ./scripts/serverless.sh remove",
    "serverless-package": "bash ./scripts/serverless.sh package",
    "serverless-s3sync": "bash ./scripts/serverless.sh s3sync",
    "output-versions": "bash ./scripts/output-versions.sh",
    "install-all": "npm install yargs && ./scripts/install.js",
    "terraform": "./scripts/terraform.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:open-remote": "./scripts/cypress_open_remote.sh",
    "cypress:run-remote": "./scripts/cypress_run_remote.sh",
    "cleanup:env": "./scripts/cleanup_env.sh",
    "remapping-es": "./scripts/remapping_es.sh",
    "analyze": "source-map-explorer 'dist/*.js'",
    "migrate:up": "cd infrastructure/db-migrations; node scripts/migrate",
    "migrate:create": "cd infrastructure/db-migrations; DB_DRIVER=$(pwd)/db-migrate-graphql npx db-migrate create --env=env --migration-table=db-migrate --config=./config.json --migrations-dir=./migrations",
    "migrate:check": "cd infrastructure/db-migrations; DB_DRIVER=$(pwd)/db-migrate-graphql npx db-migrate check --env=env --migration-table=db-migrate --config=./config.json --migrations-dir=./migrations"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@aws-amplify/api": "^1.0.39",
    "@aws-amplify/auth": "^1.2.28",
    "@aws-amplify/core": "^1.0.29",
